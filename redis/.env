# Port inside the container (host mapping is set at docker run)
REDIS_PORT=6379

# Optional password: comment out to disable auth
# REDIS_PASSWORD=change_me

# Persistence (AOF) on by default; set to "no" to disable
REDIS_APPENDONLY=yes

# Optional memory limits
# REDIS_MAXMEMORY=256mb
# REDIS_MAXMEMORY_POLICY=allkeys-lru
